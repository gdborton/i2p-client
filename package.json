{
  "name": "i2p-client",
  "scripts": {
    "build": "rm -rf dist; tsc -p tsconfig.build.json",
    "dev": "tsc --watch -p tsconfig.build.json",
    "test": "vitest --run",
    "test:integration": "vitest --run ./test/integration",
    "test:unit": "vitest --run ./test/unit",
    "tsc": "tsc --noEmit"
  },
  "keywords": [
    "i2cp",
    "i2p",
    "sam",
    "destination",
    "router",
    "client"
  ],
  "main": "./dist/index.js",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    },
    "./utils": {
      "import": "./dist/utils/utils.js",
      "require": "./dist/utils/utils.js"
    },
    "./clients/i2cp": {
      "import": "./dist/clients/i2cp/i2cp.js",
      "require": "./dist/clients/i2cp/i2cp.js"
    },
    "./clients/sam": {
      "import": "./dist/clients/sam/sam.js",
      "require": "./dist/clients/sam/sam.js"
    },
    "./protocol/*": {
      "import": "./dist/protocol/*.js",
      "require": "./dist/protocol/*.js"
    }
  },
  "dependencies": {
    "@noble/curves": "1.8.1",
    "@noble/ed25519": "^2.2.3",
    "@noble/hashes": "^1.7.1",
    "big-integer": "^1.6.52",
    "ecies-25519": "^1.3.1",
    "lru": "^3.1.0",
    "micro-rsa-dsa-dh": "^0.1.0",
    "rfc4648": "^1.5.4",
    "split2": "^4.2.0"
  },
  "devDependencies": {
    "@types/node": "22.10.7",
    "typed-emitter": "^2.1.0",
    "typescript": "^5.7.3",
    "vitest": "^3.1.1"
  }
}
